  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python Debugger: Current File",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "args": ["-b", "${workspaceFolder}", "-f", "debug_feature"],
      "console": "integratedTerminal",
      "postDebugTask": "remove_files"
    }
  ]
}
